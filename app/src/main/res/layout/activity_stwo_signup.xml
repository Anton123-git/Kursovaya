<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:fitsSystemWindows="true"
    tools:context=".STwoSignUp_Activity">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="25dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <TextView
            android:id="@+id/text9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto_medium"
            android:text="@string/text9"
            android:textColor="#000000"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/text10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:fontFamily="@font/roboto_medium"
            android:text="@string/text10"
            android:textColor="#A7A7A7"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/text11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/roboto_medium"
            android:text="@string/text11"
            android:textColor="#A7A7A7"
            android:textSize="12sp" />

        <EditText
            android:id="@+id/editTextText2"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/inputstyle"
            android:ems="10"
            android:fontFamily="@font/roboto_medium"
            android:hint="@string/text19"
            android:inputType="text"
            android:paddingLeft="10dp"
            android:textColor="#000000"
            android:textColorHint="#CFCFCF"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/text12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/roboto_medium"
            android:text="@string/text12"
            android:textColor="#A7A7A7"
            android:textSize="12sp" />

        <EditText
            android:id="@+id/editTextPhone"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/inputstyle"
            android:ems="10"
            android:fontFamily="@font/roboto_medium"
            android:hint="@string/text16"
            android:inputType="phone"
            android:paddingLeft="10dp"
            android:textColorHint="#CFCFCF"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/text13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/roboto_medium"
            android:text="@string/text13"
            android:textColor="#A7A7A7"
            android:textSize="12sp" />

        <EditText
            android:id="@+id/editTextTextEmailAddress"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/inputstyle"
            android:ems="10"
            android:fontFamily="@font/roboto_medium"
            android:hint="@string/text17"
            android:inputType="textEmailAddress"
            android:paddingLeft="10dp"
            android:textColorHint="#CFCFCF"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/text14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/roboto_medium"
            android:text="@string/text14"
            android:textColor="#A7A7A7"
            android:textSize="12sp" />

        <EditText
            android:id="@+id/editTextTextPassword"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/inputstyle"
            android:drawableRight="@drawable/icon_stpass"
            android:ems="10"
            android:fontFamily="@font/roboto_medium"
            android:hint="@string/text18"
            android:inputType="textPassword"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:textColorHint="#CFCFCF"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/text15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/roboto_medium"
            android:text="@string/text15"
            android:textColor="#A7A7A7"
            android:textSize="12sp" />

        <EditText
            android:id="@+id/editTextTextPassword2"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/inputstyle"
            android:drawableRight="@drawable/icon_stpass"
            android:ems="10"
            android:fontFamily="@font/roboto_medium"
            android:hint="@string/text18"
            android:inputType="textPassword"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:textColorHint="#CFCFCF"
            android:textSize="12sp" />

        <CheckBox
            android:id="@+id/checkBox"
            android:layout_width="match_parent"
            android:layout_height="43dp"
            android:layout_marginTop="20dp"
            android:buttonTint="@color/colorPrimary"
            android:fontFamily="@font/roboto"
            android:paddingTop="6dp"
            android:text="@string/text20"
            android:textAlignment="center"
            android:textColor="#A7A7A7" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btn_signup"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:background="@drawable/btn_signup_gray"
            android:backgroundTint="#0560FA"
            android:fontFamily="@font/roboto_bold"
            android:onClick="onClickNextAct"
            android:text="@string/btnsignup"
            android:textAllCaps="false"
            android:textColor="#FFFFFF"
            android:textSize="14sp"
            app:cornerRadius="4.69dp" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="20dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/text30"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:contentDescription="@string/text7"
                android:fontFamily="@font/roboto"
                android:text="@string/text7" />

            <TextView
                android:id="@+id/signInBtn"
                android:layout_width="48dp"
                android:layout_height="20dp"
                android:contentDescription="@string/text8"
                android:fontFamily="@font/roboto"
                android:onClick="onClickNextAct"
                android:text="@string/text8"
                android:textColor="#0560F9" />
        </LinearLayout>

        <TextView
            android:id="@+id/text16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/roboto"
            android:text="@string/text21" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            app:srcCompat="@drawable/ic_google" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>